{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Tracelet",
  "action": {
    "default_popup": "index.html",
    "default_icon": "bookmark-icon.png"
  },
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["viewer.html", "viewer.js", "viewer.css"],
      "matches": ["<all_urls>"]
    }
  ],

  "icons": {
    "128": "bookmark-icon.png"
  }
}
